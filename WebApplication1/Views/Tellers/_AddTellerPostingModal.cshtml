@model WebApplication1.ViewModels.TellerPostingViewModel
<div id="myAddTellerPostingModal" class="modal ">

    <!-- Modal content -->
    <div class="modal-content animated ">
        <span class="close">&times;</span>
        <div>
            <div class=" padding-horizontal bordered-bottom align-left">
                <h3 class="alt-color bold-text">Add Teller </h3>
            </div>
            <div class="alert alert-danger" id="errorMessage"></div>
            <div class="alert alert-success" id="successMessage"></div>
           
            <form id="tellerPostingForm">
                <div class="row form-group ">
                    <div class="margin-vertical">
                        <div class="col-md-3">
                            <label></label>
                        </div>
                        <div class="col-md-9">
                            
                        </div>
                    </div>
                </div>
                <div class="row form-group ">
                    <div class="margin-vertical">
                        <div class="col-md-3">
                            <label></label>
                        </div>
                        <div class="col-md-9">
                            
                        </div>
                    </div>
                </div>
                <div class="row form-group ">
                    <div class="margin-vertical">
                        <div class="col-md-3">
                            <label>Amount</label>
                        </div>
                        <div class="col-md-9">
                            <input type="number" id="amount">
                        </div>
                    </div>
                </div>
                <div class="row form-group ">
                    <div class="margin-vertical">
                        <div class="col-md-3">
                            <label>Narration</label>
                        </div>
                        <div class="col-md-9">
                            <input type="text" id="narration">
                        </div>
                    </div>
                </div>
                
            </form>
        </div>
    </div>

</div>





